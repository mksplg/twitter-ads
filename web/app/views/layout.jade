!!! 5
html(ng-app="twitterAds")
	head
		meta(charset='utf8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		base(href='/')
		title Twitter Ads
		link(rel='stylesheet', href='/css/main.css')
		link(rel='stylesheet', href='//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css')
		link(rel='stylesheet', href='/css/bootstrap/sticky-footer-navbar.css')
	body
		#wrap
			.navbar.navbar-default.navbar-static-top
				.container
					.navbar-header
						button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
							span.icon-bar
							span.icon-bar
							span.icon-bar
						a.navbar-brand(href='#') Twitter Ads
					.collapse.navbar-collapse
						ul.nav.navbar-nav
							- var obj = { 'users':'Users', '/users/influential':'Infuential' }
								- each val, key in obj
									- if (id == key)
										li.active
											a(href='#{key}') #{val}
									- else
										li
											a(href='#{key}') #{val}

			.container
				.starter-template
					block content

		#footer
			.container
				.text-muted.text-center.credit
					| Twitter Ads v
					span(app-version)

		script(src='js/vendor/requireJs.js')
		script(src='js/vendor/angular.js')
		script(src='js/vendor/angular-sanitize.js')
		script(src='js/vendor/angular-route.js')
		script(src='js/main.js')
		script(src='js/app.js')
		script(src='js/services.js')
		script(src='js/controllers.js')
		script(src='js/filters.js')
		script(src='js/directives.js')